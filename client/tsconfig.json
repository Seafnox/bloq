{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "lib": ["es2015", "es2016", "dom"],
    "baseUrl": ".",
    "resolveJsonModule": true,
    "paths": {
      "@block/shared/*": [
        "../shared/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "./src/**/*",
    "dataFiles.d.ts",
    "pictures.d.ts",
    "shaders.d.ts",
    "sounds.d.ts",
    "../shared/**/*"
  ]
}
